<!--pages/parameterSet/parameterSet.wxml-->
<!--一级菜单设置-->
<view class="root">
  <view class="page-section parallel p-30 border-top-line" data-index="{{index}}" wx:for="{{list}}" wx:key="*this" bindtap="secondLevelSet">
    <view class="section-name">{{item.classifyName}}</view>
    <view class="section-icon">
      <image class="w-h-40" src="../../images/arrow.png"></image>
    </view> 
  </view>
  <view class="mt-100">
     <button class="nodisabled" bindtap="refresh">刷新</button>
  </view>
</view>
